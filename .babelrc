{
  "presets": ["next/babel"],
  "plugins": [
    "inline-dotenv",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "dva": "dva-no-router"
        }
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
